{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": [2, { "properties": "always" }],
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-console": 0
  }
}
